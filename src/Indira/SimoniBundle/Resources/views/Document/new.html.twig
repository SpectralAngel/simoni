{% extends "IndiraSimoniBundle::layout.html.twig" %}

{% block content %}
<div class="row-fluid">
  <div class="span12">
    <section class="page-header">
      <h1>Importar Multiples Avistamientos</h1>
    </section>
  </div>
</div>
<div class="row-fluid">
  <section class="span9">
    <form action="{{ path('document_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p class="form-actions">
            <button class="btn btn-primary" type="submit">Guardar</button>
        </p>
    </form>
    <a class="btn btn-success" href="{{ asset('bundles/indirasimoni/fauna.xls') }}" ><i class="icon-download"></i> Descargar Plantilla Fauna</a>
    <a class="btn btn-success" href="{{ asset('bundles/indirasimoni/flora.xls') }}" ><i class="icon-download"></i> Descargar Plantilla Flora</a>
  </section>
<aside class="span3">
  <nav class="sidebar">
    <ul class="nav nav-stacked nav-tabs">
      <li>
          <a href="{{ path('document') }}">
              Volver a Documentos
          </a>
      </li>
    </ul>
  </nav>
</aside>
</div>
{% endblock %}
