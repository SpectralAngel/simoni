{% extends "IndiraSimoniBundle::layout.html.twig" %}
{% block content %}
<div class="row-fluid">
  <div class="span12">
    <section class="page-header">
      <h1>Ingresar una Denuncia</h1>
    </section>
  </div>
</div>
<div class="row-fluid">
  <section class="span9">
    <form action="{{ path('denuncia_create') }}" method="post" {{ form_enctype(form) }}>
      {{ form_widget(form) }}
      <p class="form-actions">
        <button class="btn btn-primary" type="submit">
          <i class="icon-save"></i> Guardar
        </button>
      </p>
    </form>
  </section>
  <aside class="span3">
    <nav class="sidebar">
      <ul class="nav nav-stacked nav-tabs">
        <li>
          <a href="{{ path('denuncia') }}"> Regresar a Denuncias </a>
        </li>
      </ul>
    </nav>
  </aside>
</div>
{% endblock %}
